<odoo>
   <template id="products_page" inherit_id="website_sale.products" customize_show="True" name="Select Quantity">
      <xpath expr="//div[@id='wrap']" position="before">
         <section class="s_banner parallax s_parallax_is_fixed pt96 pb96 o_colored_level" data-scroll-background-ratio="1" data-snippet="s_banner" data-name="Banner">
        <span class="s_parallax_bg oe_img_bg" style="background-image: url('pet_care_website/static/img/food-2.jpg'); background-position: 20% 0;opacity: 1;"></span>
        <div class="container">
          <div class="row s_nb_column_fixed">
            <div class="col-lg-6 jumbotron rounded pt96 pb96" data-name="Box">
                    <h2 class="o_default_snippet_text">
                           For Dogs
                        </h2>
                        <p class="o_default_snippet_text color-grn">No fillers. No preservatives! <br/>
                           PawPots only uses fresh, human-grade, nutritious <br/>
                           ingredients.
                           For real!
                        </p>
<!--                <h2 class="o_default_snippet_brown_text">Sign Up to our Newsletter</h2>-->
            </div>
          </div>
        </div>
      </section>
      </xpath>
   </template>
